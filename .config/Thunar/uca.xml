<action>
	<icon>web-browser-symbolic</icon>
	<name>Slugify Name</name>
	<unique-id>1492891723337691-50</unique-id>
	<command>for file in %N; do mv &quot;$file&quot; &quot;$(echo &quot;$file&quot; | tr -s &apos; &apos; | tr &apos; A-Z&apos; &apos;-a-z&apos; | tr -s &apos;-&apos; | tr -c &apos;[:alnum:][:cntrl:].&apos; &apos;-&apos;)&quot;; done</command>
	<description>Convert name to all lowercase, replacing spaces with hyphens</description>
	<upper>-1</upper>
	<lower>-1</lower>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
